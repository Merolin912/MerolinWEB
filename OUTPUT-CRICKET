 CREATE TABLE TEAM(
 TEAMID VARCHAR(20) PRIMARY KEY,
 TEAMNAME VARCHAR(20) NOT NULL,
 COACH VARCHAR(20),
 CITY VARCHAR(20));

 ALTER TABLE TEAM
 ADD CAPTAINID VARCHAR(20);
 
 ALTER TABLE TEAM
 ADD FOREIGN KEY(CAPTAINID)REFERENCES PLAYER(PLAYERID);
 
