 CREATE TABLE PLAYER(
 PLAYERID VARCHAR(20) PRIMARY KEY,
 PLAYERNAME VARCHAR(20) NOT NULL,
 CONTACT INT(10) NOT NULL,
 AGE INT(6) NOT NULL,
 TEAMID VARCHAR(20),
 FOREIGN KEY(TEAMID)REFERENCES TEAM(TEAMID));




